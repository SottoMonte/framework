<?xml version="1.0" encoding="UTF-8"?>
<User>
    <Resources>
        <Model name="client" access="self" />
    </Resources>
    <Routes band="standard" requests_time="10s">
        <Route path="/logout" type="action" method="POST" BANDWIDTH="1000mbit/s" />
        <Route path="/" type="view" method="GET" />
        <Route path="/gather" type="action" method="GET" policy="default" />
        <Route path="/register" type="action" method="POST" />
        <Route path="/login" type="action" method="POST" />
    </Routes>
</User>