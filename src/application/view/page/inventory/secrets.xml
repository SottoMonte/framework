{% extends "sidebar.xml" %}
{% block breadcrumb %}
<Action route="application/view/page/inventory/overview.xml" ><Graph icon="bi-speedometer2"/></Action>
<Action route="application/view/page/inventory/secrets.xml" ><Graph icon="bi-key-fill"/><Text>Repository</Text></Action>
{% endblock %}
{% block title %}Overview{% endblock %}
{% block subtitle %}Keep track of your inventory{% endblock %}
{% block bar %}
<SideBar selected="application/view/page/inventory/secrets.xml"/>
{% endblock %}
{% block content %}
<Container background="#fafafa" size="fluid" padding="p-4" expand="True" class="border-top border-bottom">
    <Table repository="secrets" view="secret" >
        <Text>Field</Text>
        <Text>Secret</Text>
        <Text>Description</Text>
        <Text>Created</Text>
        <Text>Updated</Text>
        <Text>Actions</Text>
    </Table>
</Container>
{% endblock %}