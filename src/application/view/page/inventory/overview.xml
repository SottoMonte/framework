{% extends "main.xml" %}
{% block title %}Overview{% endblock %}
{% block subtitle %}Keep track of your inventory{% endblock %}
{% block bar %}
<Action route="application/view/page/orakom/dashboard.xml">
    <Graph icon="bi-view-list" />
    <Text>Overview</Text>
</Action>
<Action route="application/view/page/orakom/clients.xml">
    <Graph icon="bi-pc-horizontal" />
    <Text>Virtual machines</Text>
</Action>
<Action route="application/view/page/orakom/contracts.xml">
    <Graph icon="bi-box-seam" />
    <Text>Containers</Text>
</Action>
<Action route="application/view/page/orakom/invoices.xml">
    <Graph icon="bi-boxes" />
    <Text>Services</Text>
</Action>
<Action route="application/view/page/orakom/invoices.xml">
    <Graph icon="bi-globe" />
    <Text>Domains</Text>
</Action>
<Action route="application/view/page/orakom/invoices.xml">
    <Graph icon="bi-database" />
    <Text>Volumes</Text>
</Action>
<Action route="application/view/page/orakom/invoices.xml">
    <Graph icon="bi-braces-asterisk" />
    <Text>Software</Text>
</Action>
<Action route="application/view/page/orakom/invoices.xml">
    <Graph icon="bi-file-ruled" />
    <Text>Politics</Text>
</Action>
{% endblock %}
{% block content %}
<Container color="#fafafa" size="fluid" padding="p-4" expand="True" class="border-top border-bottom">
    <Input disabled="True"></Input>
</Container>
{% endblock %}