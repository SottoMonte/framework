</Route>
<Route url="application/view/page/orakom/invoices.xml">
    <Text><Icon icon="bi-database" color="white" />Volumes</Text>
</Route>
<Route url="application/view/page/orakom/invoices.xml">
    <Text><Icon icon="bi-braces-asterisk" color="white" />Software</Text>
</Route>
