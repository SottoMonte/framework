{% extends "sidebar.xml" %}
{% block title %}Overview{% endblock %}
{% block subtitle %}Keep track of your inventory{% endblock %}
{% block bar %}
<SideBar selected="application/view/page/inventory/volumes.xml"/>
{% endblock %}
{% block content %}
<Container background="#fafafa" size="fluid" padding="p-4" expand="True" class="border-top border-bottom">
    <Table repository="volumes" view="volume" >
        <Text>Nome</Text>
        <Text>Description</Text>
        <Text>Type</Text>
        <Text>Driver</Text>
        <Text>Snapshot</Text>
        <Text>Actions</Text>
    </Table>
</Container>
{% endblock %}