{% extends "cardinal.xml" %}
{% block west %}
<Column size="expanded">
    <!-- TabNav -->
    <Group type="nav" margin="m-0" padding="p-0" color="#ededed" class="flex-column">
        <Action link="#tab-explore" data-bs-toggle="tab"><Graph icon="bi-archive-fill"/><Text>Explore</Text></Action>
        <Action link="#tab-tool" data-bs-toggle="tab"><Graph icon="bi-tools"/><Text>Tools</Text></Action>
        <Action link="#tab-copilot" data-bs-toggle="tab"><Graph icon="bi-stars"/><Text>Copilot</Text></Action>
    </Group>
    <!-- Tab -->
    <Group type="tab">
        <Container id="tab-tool" size="full" class="border-start">
            <Column id="box" class="flex-wrap" style="font-size: 40px;" >
                <Action id="maker-window" draggable="window" draggable-type="window" >
                    <Graph icon="bi-link" />
                </Action>
                <Action id="maker-block" draggable="element" draggable-type="block" >
                    <Graph icon="bi-send" />
                </Action>
                <Action id="img34" draggable="window"  >
                    <Graph icon="bi-image" />
                </Action>
                <Action id="img499" draggable="window" >
                    <Graph icon="bi-window" />
                </Action>
                <Action id="img44" draggable="window" >
                    <Graph icon="bi-layout-three-columns" />
                </Action>
                <Action id="img33" draggable="window" >
                    <Graph icon="bi-textarea" />
                </Action>
                <Action id="img3" draggable="window" >
                    <Graph icon="bi-textarea-resize" />
                </Action>
            </Column>
        </Container>
        <Container id="tab-explore" size="full" class="border-start">
            <Group type="tree" >
                <Group type="node" margin="m-0" padding="p-0" color="#ededed">
                    Application
                    <Group type="node" margin="m-0" padding="p-0" color="#ededed">
                        Action
                        <Text id="maker-window-2" draggable="window" draggable-type="window" value="application/action/close.py" >close.py</Text>
                        <Text id="maker-2" draggable="window" draggable-type="window" value="application/action/load.py" >load.py</Text>
                    </Group>
                    <Group type="node" margin="m-0" padding="p-0" color="#ededed">
                        <Text id="ss" draggable="window" draggable-type="window" >overview.xml</Text>
                        <Text id="sss" draggable="window" draggable-type="window" >overview.xml</Text>
                    </Group>
                </Group>
            </Group>
        </Container>
        <Container id="tab-copilot" size="full" class="border-start">
            <Text>3</Text>
        </Container>
    </Group>
</Column>
{% endblock %}
{% block north %}
{% endblock %}
{% block center %}
<Row size="expanded" class="overflow-auto">
    <Group type="nav" margin="m-0" padding="p-0" color="#ededed">
        <Action link="#framework" data-bs-toggle="tab"><Graph icon="bi-pass"/><Text>framework</Text></Action>
        <Action link="#infrastructure" data-bs-toggle="tab"><Graph icon="bi-receipt"/><Text>infrastructure</Text></Action>
    </Group>
    <Group type="tab" size="expanded" class="overflow-auto">
        <Column id="framework" class="overflow-auto" size="full">
            <Row droppable="window" class="col overflow-auto">
                    
            </Row>
            <Row droppable="window" class="col overflow-auto">
                
            </Row>
        </Column>
        <Container id="infrastructure" size="full">
            <Text>3</Text>
        </Container>
    </Group>
</Row>
{% endblock %}
{% block south %}
<Container>
    <!-- TabNav -->
    <Group type="nav" margin="m-0" padding="p-0" color="#ededed">
        <Action link="#tab-code" data-bs-toggle="tab"><Graph icon="bi-code-slash"/><Text>Code</Text></Action>
        <Action link="#tab-build" data-bs-toggle="tab"><Graph icon="bi-tools"/><Text>Build</Text></Action>
        <Action link="#tab-test" data-bs-toggle="tab"><Graph icon="bi-play-fill"/><Text>Test</Text></Action>
        <Action link="#tab-release" data-bs-toggle="tab"><Graph icon="bi-rocket-fill"/><Text>Release</Text></Action>
    </Group>
    <!-- Tab -->
    <Group type="tab">
        <Container id="tab-code">
            <Text>1</Text>
        </Container>
        <Container id="tab-build">
            <Text>2</Text>
        </Container>
        <Container id="tab-test">
            <Text>3</Text>
        </Container>
        <Container id="tab-release">
            <Text>4</Text>
        </Container>
    </Group>
</Container>
{% endblock %}
{% block east %}
<Group type="tree" >
    <Group type="node" margin="m-0" padding="p-0" color="#ededed">
        window
        <Group type="node" margin="m-0" padding="p-0" color="#ededed">
            column
            <Text id="maker-window-2" draggable="window" draggable-type="window" >text.html</Text>
            <Text id="maker-2" draggable="window" draggable-type="window" >overview.xml</Text>
        </Group>
        <Group type="node" margin="m-0" padding="p-0" color="#ededed">
            row
            <Text id="ss" draggable="window" draggable-type="window" >text</Text>
            <Text id="sss" draggable="window" draggable-type="window" >text</Text>
        </Group>
    </Group>
</Group>
<!-- TabNav -->
<Group type="nav" margin="m-0" padding="p-0" color="#ededed">
    <Action link="#tab-property" data-bs-toggle="tab"><Graph icon="bi-sliders2"/><Text>Property</Text></Action>
    <Action link="#tab-style" data-bs-toggle="tab"><Graph icon="bi-rulers"/><Text>Style</Text></Action>
    <Action link="#tab-event" data-bs-toggle="tab"><Graph icon="bi-lightning-fill"/><Text>Event</Text></Action>
</Group>
<!-- Tab -->
<Group type="tab">
    <Container id="tab-property">
        <Text>1</Text>
    </Container>
    <Container id="tab-style">
        <Text>2</Text>
    </Container>
    <Container id="tab-event">
        <Text>3</Text>
    </Container>
</Group>
{% endblock %}