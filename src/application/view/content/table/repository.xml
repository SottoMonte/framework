        <Action click="ide(id:'ide',repository:{'owner':'{{ storekeeper.owner.login }}','name':'{{ storekeeper.name }}'})" route="application/view/page/cms/overview.xml" class="d-inline-flex"><Graph icon="bi-box-fill"/></Action>
        <Action route="application/view/page/inventory/repository.xml" class="d-inline-flex"><Graph icon="bi-bookmark-fill"/></Action>
    </Group>
</Graph>