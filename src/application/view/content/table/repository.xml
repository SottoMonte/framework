<Graph type="table.row">
    <Text storekeeper="name">name</Text>
    <Text storekeeper="description">description</Text>
    <Text storekeeper="visibility">type</Text>
    <Text>Github</Text>
    <Text storekeeper="language">language</Text>
    <Text storekeeper="updated">Updated</Text>
